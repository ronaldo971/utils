<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.1"
    STATUS="ALPHA"    
    COMPATIBILITY="ALL"
    AUTHOR="AUGUSTO Manuel"
    RELEASE_DATE="20120110A"
    STARTABLE="false"    
    SCHEMA_AUTO_PROPAGATE="false"  
  >
  
  <!--
  	HAS_CONDITIONAL_OUTPUTS="false"
    DATA_AUTO_PROPAGATE="false"   
   -->
    <SIGNATURE></SIGNATURE>
  </HEADER>
  
  <FAMILIES>
    <FAMILY>Processing</FAMILY>
  </FAMILIES>  

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

   <PARAMETERS>
    <PARAMETER NAME="SCHEMA" 
    	FIELD="SCHEMA_TYPE" 
    	REQUIRED="true" 
    	NUM_ROW="1">
      <DEFAULT/> 
    </PARAMETER>

    <PARAMETER
      NAME="SUBSTITUTIONS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="3"
      NB_LINES="8"
    >
      <ITEMS>      
       	<ITEM NAME="INPUT_COLUMN" FIELD="COLUMN_LIST" />
		<ITEM NAME="SPLIT_RIGHT" FIELD="CHECK" VALUE="true" />
		<ITEM NAME="SPLIT_LEFT" FIELD="CHECK" VALUE="false" />		
      </ITEMS>
	  
    </PARAMETER>
            
   </PARAMETERS>

   <ADVANCED_PARAMETERS/>

	<CODEGENERATION>
	  <IMPORTS>
	  	<IMPORT MODULE="commons-lang-2.6.jar" NAME="commons-lang-2.6" REQUIRED="true"/> 	
	  </IMPORTS>
	</CODEGENERATION>

   <RETURNS>
     <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER"/>
     <RETURN NAME="NB_LINE_OK" TYPE="INTEGER" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>